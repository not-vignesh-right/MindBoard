{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "memory": 1024
    }
  },
  "env": {
    "NODE_ENV": "production",
    "FORCE_OFFLINE_MODE": {
      "development": "true",
      "preview": "false",
      "production": "false"
    }
  }
}